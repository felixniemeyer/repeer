{
  "manifest_version": 2,
  "name": "repeer",
  "version": "0.1",

  "description": "shows agent reputation based on personal experiences",

  "icons": {
  },

  "content_scripts": [
    {
      "matches": ["*"],
      "js": ["content.js"]
    }
  ], 

  "background": {
    "scripts": ["background.js"]
  }, 

  "browser_action": {
    "default_title": "repeer",
    "default_icon": "popup/icon.png", 
    "default_popup": "popup/index.html"
  }
}
